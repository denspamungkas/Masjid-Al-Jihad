    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/recycler_edit_adha">

    </androidx.recyclerview.widget.RecyclerView>

</LinearLayout>